[program:<%= @program.name %>]
command = <%= @program.command %>
<% if not @program.autostart.nil? -%>
autostart = <%= @program.autostart %>
<% end -%>
<% if not @program.autorestart.nil? -%>
autorestart = <%= @program.autorestart %>
<% end -%>
<% if not @program.startsecs.nil? -%>
startsecs = <%= @program.startsecs %>
<% end -%>
<% if not @program.startretries.nil? -%>
startretries = <%= @program.startretries %>
<% end -%>
<% if not @program.user.nil? -%>
user = <%= @program.user %>
<% end -%>
<% if not @program.directory.nil? -%>
directory = <%= @program.directory %>
<% end -%>
<% if not @program.priority.nil? -%>
priority = <%= @program.priority %>
<% end -%>

<% if not @program.redirect_stderr.nil? -%>
redirect_stderr = <%= @program.redirect_stderr %>
<% end -%>
<% if not @program.stdout_logfile.nil? -%>
stdout_logfile = <%= @program.stdout_logfile %>
<% end -%>
<% if not @program.stdout_logfile_maxbytes.nil? -%>
stdout_logfile_maxbytes = <%= @program.stdout_logfile_maxbytes %>
<% end -%>
<% if not @program.stderr_logfile.nil? -%>
stderr_logfile = <%= @program.stderr_logfile %>
<% end -%>
<% if not @program.stderr_logfile_maxbytes.nil? -%>
stderr_logfile_maxbytes = <%= @program.stderr_logfile_maxbytes %>
<% end -%>
<% if not @program.stopsignal.nil? -%>

stopsignal = <%= @program.stopsignal %>
<% end -%>
<% if not @program.environment.empty? -%>

environment = <%= @program.environment.map{|k,v| "#{k}=\"#{v}\""}.join(',') %>
<% end -%>
